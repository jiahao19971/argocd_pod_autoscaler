{
    "argocd": {
        "required": true,
        "type": "dict",
        "schema": {
            "username": {
                "required": true,
                "type": "string"
            },
            "password": {
                "required": true,
                "type": "string"
            }
        }
    },
    "aws": {
        "required": false,
        "type": "dict",
        "schema": {
            "aws_access_key_id": {
                "required": true,
                "type": "string"
            },
            "aws_secret_access_key": {
                "required": true,
                "type": "string"
            },
            "region_name": {
                "required": true,
                "type": "string"
            }
        }
    },
    "slack":{
        "required": false,
        "type": "dict",
        "schema":{
            "token":{
                "required": true,
                "type": "string"
            }
        }
    }
}